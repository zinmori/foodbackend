{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { 
      "src": "/(.*)", 
      "dest": "index.js"
    },
    { 
      "src": "/meals", 
      "dest": "index.js"
    },
    { 
      "src": "/orders", 
      "dest": "index.js"
    }
  ]
}